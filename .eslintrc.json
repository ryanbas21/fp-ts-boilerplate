{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2017,
    "project": "tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint",
    "import",
    "jest"
  ],
  "extends": [
    "airbnb-typescript",
    "plugin:jest/recommended"
  ],
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    },
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true
      },
      "typescript": {}
    }
  },
    "rules": {
      "no-dupe-class-members": "off",
      "@typescript-eslint/no-dupe-class-members": "off"
  }
}
